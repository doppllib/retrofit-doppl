# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY START - can be moved as a block
def j2objc_retrofit
    pod 'j2objc-retrofit-debug', :configuration => ['Debug'], :path => '../retrofit/build/doppel'
    pod 'j2objc-retrofit-release', :configuration => ['Release'], :path => '../retrofit/build/doppel'
    pod 'j2objc-retrofit-test', :path => '../retrofit/build/j2objcOutputs'
end
# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY END

target 'ios' do
    platform :ios, '8.3'
